<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="566.0" prefWidth="1287.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sales_department.controller.contract.ViewContract">
   <children>
      <TableView fx:id="viewContractTableView" layoutX="265.0" layoutY="115.0" prefHeight="368.0" prefWidth="997.0">
        <columns>
            <TableColumn fx:id="statusTableColumn" prefWidth="54.0" text="Статус договора" />
          <TableColumn fx:id="numberSpecificationTableColumn" prefWidth="131.0" text="Номер приложения" />
          <TableColumn fx:id="dataConclusionTableColumn" prefWidth="118.0" text="Дата заключения" />
            <TableColumn fx:id="dataStartTableColumn" prefWidth="118.0" text="Дата вступления договора в силу" />
            <TableColumn fx:id="dataEndTableColumn" prefWidth="121.0" text="Договор действителен до" />
            <TableColumn fx:id="ContractorTableColumn" prefWidth="208.0" text="Контрагент" />
            <TableColumn fx:id="cityTableColumn" prefWidth="90.0" text="Город заключения договора" />
            <TableColumn fx:id="adressTableColumn" prefWidth="156.0" text="Адрес грузополучателя" />
        </columns>
      </TableView>
      <Button fx:id="exitButton" layoutX="21.0" layoutY="406.0" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="77.0" prefWidth="194.0" text="Выход" />
      <Text fx:id="viewContractText" layoutX="35.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Просмотр договора" />
      <Button fx:id="editContractButton" layoutX="21.0" layoutY="205.0" mnemonicParsing="false" onAction="#onEditContractButtonClick" prefHeight="85.0" prefWidth="194.0" text="Найти существующий договор" />
      <Button fx:id="addContractButton" layoutX="21.0" layoutY="115.0" mnemonicParsing="false" onAction="#onAddContractButtonClick" prefHeight="68.0" prefWidth="194.0" text="Добавить договор" />
      <Button disable="true" layoutX="21.0" layoutY="305.0" mnemonicParsing="false" prefHeight="85.0" prefWidth="194.0" text="Реестр" />
   </children>
</AnchorPane>
